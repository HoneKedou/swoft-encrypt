{
    "name": "zcmzc/swoft-encrypt",
    "type": "library",
    "keywords": [
        "php",
        "swoft",
        "encrypt"
    ],
    "description": "Swoft 加密/签名组件",
    "license": "Apache-2.0",
    "require": {
        "ext-openssl": "*",
        "ext-json": "*",
        "swoft/http-server": "^1.0",
        "swoft/http-message": "^1.0",
        "swoft/framework": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Encrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Encrypt\\": "test/Cases",
            "SwoftTest\\Testing\\": "test/Testing"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    ],
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
